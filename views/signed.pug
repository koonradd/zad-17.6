doctype html
html(lang='pl')
    head
        meta(charset='UTF-8')
        title="Logowanie"
    body
        form(method='GET' action='/register')
            p.form-text
            | Imię: 
            input(type='text', name='name', placeholder='Imię' )
            p.form-text
            | Email: 
            input(type='email', name='email', placeholder='name@email.com')
            input.submit(type='submit', value='Submit')